	},

	['circa2'] = {
		nDates = 2,
		ar='حوالي $date1 – $date2',
		['be-tarask']={'прыкладна з $date1 да прыкладна $date2', case={'gen','gen'}},
		bn='প্রায় $date1 এবং প্রায় $date2-এর মাঝামাঝি',
