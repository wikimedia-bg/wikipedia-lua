		['en-gb']=function(date1, state) return Ordinal(date1, 'en-gb') .. ' millennium' end,
		['en-ca']=function(date1, state) return Ordinal(date1, 'en-ca') .. ' millennium' end,
		eo='$date1-a jarmilo',
