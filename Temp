local p = {}

local function getVal(obj)
	local result = ''
	for i, v in pairs(obj) do
	    if i ~= 'test' then
	        result = result .. '\n' .. i .. ': ' .. v
	    end
	end

	local root = mw.html.create('div')
	root:wikitext(result)
 
	return tostring(root)
end

function p.test(frame)
	p.code = mw.language.getContentLanguage().code
	p.lang = 'lang' --mw.language.new(code)
	
	return getVal(p)
end

return p