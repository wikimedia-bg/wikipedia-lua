local p = {}

local function getVal(obj)
	local root = mw.html.create('div')
	for i, v in pairs(obj) do
		local newDiv = root:tag('div')
		newDiv:wikitext(i .. ': ' .. tostring(v))
	end
	
	return tostring(root)
end

function p.test()

	p.xxx = mw.wikibase.getEntityObject(nil)
	
	return getVal(p.xxx)
end

return p