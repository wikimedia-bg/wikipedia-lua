local p = {}

local function getVal(obj)
	local root = mw.html.create('div')
	for i, v in pairs(obj) do
		local newDiv = root:tag('div')
		newDiv:wikitext(i .. ': ' .. tostring(v))
	end
	
	return tostring(root)
end

function p.test(frame)
	local wiki = 
	{
		langcode = mw.language.getContentLanguage().code
	}
	
	p.lang = mw.language.new(wiki.langcode):formatDate('Y', '1768-00-00T00:00:00Z')
	
	return getVal(p.lang)
end

return p